{
  "include": ["env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "~/*": ["./app/*"],
      "storefrontapi.generated": ["./storefrontapi.generated.d.ts"],
      "customer-accountapi.generated": ["./customer-accountapi.generated.d.ts"]
    },
    "rootDirs": ["."],
    "types": ["vite/client"]
  }
}
